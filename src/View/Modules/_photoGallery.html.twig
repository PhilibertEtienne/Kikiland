{% extends 'Modules/layout.html.twig' %}

{% block head %}
	{{ parent() }}
	<link rel="stylesheet" href="assets/css/tattoo.css" type="text/css">
{% endblock %}

{% block content %}
	<div class="grid">
		{% for image in images %}
			<img src="/assets/Images/{{ folder }}/{{ image }}" alt="{{ image }}" class="grid-image">
		{% endfor %}
	</div>
{% endblock %}